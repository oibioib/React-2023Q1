{
  "name": "react-2023q1",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "dev:ts": "ts-node src/server.ts",
    "build": "npm run clean:dist && npm run lint:check && npm run build:server && npm run build:app:client && npm run build:app:server",
    "build:app:client": "vite build --outDir dist/client",
    "build:app:server": "vite build --ssr src/AppServer.tsx --outDir dist/server",
    "build:server": "vite build --outDir dist --config vite.config.server.ts",
    "start": "set \"NODE_ENV=production\" && node dist/server.js",
    "test": "npm run test:unit",
    "test:unit": "vitest --coverage",
    "test:e2e": "set \"TEST=true\" && set \"ISTANBUL=true\" && npm run clean:test && start-server-and-test dev:ts http://localhost:5173 cypress:run && npm run cypress:coverage && npm run cypress:coverage:summary",
    "test:e2e:browser": "set \"TEST=true\" && set \"ISTANBUL=true\" && npm run clean:test && start-server-and-test dev:ts http://localhost:5173 cypress:open && npm run cypress:coverage:summary",
    "cypress:run": "cypress run --e2e",
    "cypress:open": "cypress open --e2e",
    "cypress:coverage": "nyc report",
    "cypress:coverage:summary": "nyc report --reporter=text-summary",
    "clean:dist": "rimraf dist",
    "lint": "eslint ./src --ext .ts,.tsx --ignore-path ./.gitignore --max-warnings=0",
    "lint:fix": "eslint ./src --ext .ts,.tsx --ignore-path ./.gitignore --fix",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{ts,tsx,scss,css,json,md}\" --ignore-path ./.gitignore",
    "lint:check": "npm run lint && prettier --check . --ignore-path ./.gitignore",
    "clean:test": "rimraf coverage && rimraf .nyc_output",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "npm run lint:fix"
    ],
    "*.(ts|tsx|scss|css|json|md)": [
      "npm run lint:format"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "express": "^4.18.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-remove-scroll": "^2.5.5",
    "react-router-dom": "^6.9.0",
    "sirv": "^2.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "cypress": "^12.11.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "rimraf": "^5.0.0",
    "sass": "^1.59.3",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.3",
    "whatwg-fetch": "^3.6.2"
  }
}
